{
  "name": "@motionpicture/sskts-api-javascript-client",
  "version": "4.4.0-alpha.0",
  "description": "SSKTS API client library for JavaScript",
  "main": "./lib/index.js",
  "browser": "./lib/index.js",
  "types": "./lib/index.d.ts",
  "files": [
    "doc/",
    "example/",
    "lib/"
  ],
  "directories": {
    "doc": "./doc",
    "lib": "./lib",
    "example": "./example",
    "test": "./test"
  },
  "scripts": {
    "check": "npm run tslint",
    "clean": "rimraf ./lib \"npm-debug.log*\" ./doc ./coverage ./.nyc_output",
    "build": "npm run build:node && npm run build:browser",
    "build:node": "tsc",
    "build:browser": "browserify ./browser.js -o ./lib/browser.js",
    "build:watch": "tsc && concurrently \"tsc -w\" \"npm run watchify\"",
    "watchify": "watchify ./browser.js -o ./lib/browser.js",
    "nyc": "nyc mocha \"src/**/*.spec.ts\"",
    "coverage": "npm run nyc | coveralls",
    "test": "npm run check && npm run coverage",
    "snyk": "snyk wizard",
    "doc": "jsdoc -c jsdoc.json",
    "example": "copyfiles ./lib/browser.js ./example/browser && http-server ./example/browser",
    "tslint": "tslint --project tsconfig.json -c tslint.json --exclude \"**/*.spec.ts\" \"src/**/*.ts\"",
    "preversion": "npm run clean && npm run build && npm test",
    "version": "git add -A",
    "postversion": "git push origin --tags",
    "prepublishOnly": "npm run clean && npm run build && npm test && npm run doc"
  },
  "author": "Motionpicture co.,ltd.",
  "contributors": [
    "Tetsu Yamazaki <yamazaki@motionpicture.jp>"
  ],
  "license": "ISC",
  "devDependencies": {
    "@types/mocha": "^2.2.48",
    "browserify": "^14.5.0",
    "concurrently": "^3.6.1",
    "copyfiles": "^1.2.0",
    "coveralls": "^2.13.3",
    "fs-extra": "^4.0.3",
    "http-server": "^0.10.0",
    "ink-docstrap": "^1.3.2",
    "jsdoc": "^3.5.5",
    "jsonwebtoken": "^7.4.3",
    "jwk-to-pem": "^1.2.6",
    "mocha": "^3.5.3",
    "nyc": "^12.0.2",
    "rimraf": "^2.6.2",
    "snyk": "^1.116.4",
    "ts-node": "^6.2.0",
    "tslint": "^5.11.0",
    "tslint-microsoft-contrib": "^5.2.1",
    "typescript": "^3.2.4",
    "watchify": "^3.11.0"
  },
  "dependencies": {
    "@cinerino/api-javascript-client": "^1.0.0-alpha.28",
    "@motionpicture/sskts-api-abstract-client": "^4.5.0-alpha.2"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/motionpicture/sskts-api-javascript-client.git"
  }
}
